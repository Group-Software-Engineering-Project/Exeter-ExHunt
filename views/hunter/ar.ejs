<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>ExHunt</title>
    <script src="https://cdn.jsdelivr.net/gh/aframevr/aframe@1c2407b26c61958baa93967b5412487cd94b290b/dist/aframe-master.min.js"></script>
    <script src='https://raw.githack.com/jeromeetienne/AR.js/master/aframe/build/aframe-ar.min.js'></script>
    <script>
        THREEx.ArToolkitContext.baseURL = 'https://raw.githack.com/jeromeetienne/ar.js/master/three.js/'
    </script>
</head>

<!-- include GeoAR.js -->
<script src="../javascripts/gps-entity-place.js"></script>
<script src="../javascripts/gps-camera-debug.js"></script>
<script src="../javascripts/gps-camera.js"></script>
<link rel="stylesheet" href="../stylesheets/index.css">

<!-- Dynamically add places from Javascript -->
<script src="../javascripts/places.js"></script>

<body style="margin: 0; overflow: hidden;">
    <a-scene
        cursor="rayOrigin: mouse; fuse: true; fuseTimeout: 0;"
        raycaster="objects: [gps-entity-place];"
        vr-mode-ui="enabled: true;">
        <a-camera gps-camera="minDistance: 10;" rotation-reader>
        </a-camera>
        <a-box color="yellow" gps-entity-place="latitude: 50.737344; longitude: -3.532559"/>
        <a-box color="yellow" gps-entity-place="[gps-entity-place]"/>
        <a-box color="yellow" raycaster/>
	</a-scene>
</body>

